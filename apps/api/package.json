{
  "name": "turborepo-starter-kit-api",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "john.chen",
    "url": "https://github.com/john-data-chen/turborepo-starter-kit"
  },
  "scripts": {
    "dev": "rspack dev --watch",
    "build": "cross-env BUILD=true rspack build",
    "start": "node dist/main.js",
    "init-db": "tsx database/init-db-data.ts",
    "init-db:force": "tsx database/init-db-data.ts --force",
    "lint": "npx oxlint --config ../../packages/linter-config/.oxlintrc.json --fix",
    "format": "cd ../.. && oxfmt --ignore-path .prettierignore --no-error-on-unmatched-pattern --config .oxfmtrc.json apps/api",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/swagger": "^11.2.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "*",
    "mongoose": "^9.1.3",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "run-script-webpack-plugin": "^0.2.3",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0",
    "vite-tsconfig-paths": "^6.0.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@rspack/cli": "^1.7.2",
    "@rspack/core": "^1.7.2",
    "@swc/cli": "^0.7.10",
    "@swc/core": "^1.15.8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "*",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/node": "*",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "*",
    "cross-env": "^10.1.0",
    "globals": "^17.0.0",
    "jsdom": "*",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "*",
    "vitest": "*"
  },
  "engines": {
    "node": ">=24.11.0"
  }
}