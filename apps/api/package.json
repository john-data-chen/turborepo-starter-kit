{
  "name": "turborepo-starter-kit-api",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "john.chen",
    "url": "https://github.com/john-data-chen/turborepo-starter-kit"
  },
  "scripts": {
    "dev": "rspack dev",
    "build": "cross-env BUILD=true rspack build",
    "start": "node dist/main.js",
    "db:init": "tsx database/init-db-data.ts",
    "db:init:force": "tsx database/init-db-data.ts --force",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "oxlint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "dotenv": "^16.6.1",
    "mongoose": "^8.16.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "run-script-webpack-plugin": "^0.2.3",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.5",
    "@rspack/cli": "^1.4.10",
    "@rspack/core": "^1.4.10",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.13.2",
    "@types/express": "^5.0.3",
    "@types/node": "^22.16.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "global-tsconfig": "workspace:*",
    "globals": "^15.15.0",
    "linter-config": "workspace:*",
    "oxlint": "^1.8.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "style-formatter-config": "workspace:*",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unplugin-swc": "^1.5.5",
    "vitest": "^3.2.4"
  }
}