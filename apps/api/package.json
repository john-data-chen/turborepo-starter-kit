{
  "name": "turborepo-starter-kit-api",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "john.chen",
    "url": "https://github.com/john-data-chen/turborepo-starter-kit"
  },
  "scripts": {
    "build": "cross-env BUILD=true rspack build",
    "dev": "rspack dev",
    "format": "cd ../.. && oxfmt --no-error-on-unmatched-pattern --config .oxfmtrc.json apps/api",
    "init-db": "tsx database/init-db-data.ts",
    "init-db:force": "tsx database/init-db-data.ts --force",
    "lint": "npx oxlint --config ../../.oxlintrc.json --fix",
    "start": "node dist/main.js",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.13",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.13",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.13",
    "@nestjs/swagger": "^11.2.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "catalog:",
    "mongoose": "^9.1.5",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "run-script-webpack-plugin": "^0.2.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.13",
    "@rspack/cli": "^1.7.5",
    "@rspack/core": "^1.7.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/node": "catalog:",
    "@types/passport-jwt": "^4.0.1",
    "@vitest/coverage-v8": "catalog:",
    "cross-env": "^10.1.0",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": ">=24.13.0"
  }
}