{
  "name": "turborepo-starter-kit-api",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "john.chen",
    "url": "https://github.com/john-data-chen/turborepo-starter-kit"
  },
  "scripts": {
    "dev": "rspack dev --watch",
    "build": "cross-env BUILD=true rspack build",
    "start": "node dist/main.js",
    "init-db": "tsx database/init-db-data.ts",
    "init-db:force": "tsx database/init-db-data.ts --force",
    "lint": "npx oxlint --config ../../packages/linter-config/.oxlintrc.json --fix",
    "format": "prettier --write . --ignore-path ../../.prettierignore --no-error-on-unmatched-pattern --config ../../prettier.config.js",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "engines": {
    "node": ">=24.11.0"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "mongoose": "^8.19.2",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "run-script-webpack-plugin": "^0.2.3",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@rspack/cli": "^1.6.0",
    "@rspack/core": "^1.6.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.14.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.23",
    "@types/node": "^24.9.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "cross-env": "^10.1.0",
    "globals": "^16.4.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}