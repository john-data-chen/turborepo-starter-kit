{
  "name": "turborepo-starter-kit-web",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "john.chen",
    "url": "https://github.com/john-data-chen/turborepo-starter-kit"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "npx oxlint --config ../../packages/linter-config/.oxlintrc.json --fix",
    "format": "prettier --write . --ignore-path ../../.prettierignore --no-error-on-unmatched-pattern --config ../../prettier.config.js",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky; fi",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch",
    "playwright": "npx playwright test"
  },
  "engines": {
    "node": ">=22.11.0"
  },
  "packageManager": "pnpm@10.18.1",
  "dependencies": {
