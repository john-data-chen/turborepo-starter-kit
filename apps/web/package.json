    "react": "*",
    "react-dom": "*",
    "react-hook-form": "*",
    "sonner": "^2.0.7",
    "tailwindcss": "*",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "dotenv": "^17.2.3",
    "typescript": "*"
  }
