{
  "name": "@repo/ui",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components/*": {
      "types": "./src/components/ui/*.tsx",
      "import": "./src/components/ui/*.tsx"
    },
    "./lib/*": {
      "types": "./src/lib/*.ts",
      "import": "./src/lib/*.ts"
    },
    "./styles.css": "./src/styles/globals.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "lint": "npx oxlint --config ../linter-config/.oxlintrc.json",
    "format": "prettier --write . --ignore-path ../../.prettierignore --write --no-error-on-unmatched-pattern --config ../../prettier.config.js",
    "test": "vitest run --coverage"
  },
  "engines": {
    "node": ">=22.11.0"
  },
  "packageManager": "pnpm@10.17.1",
  "peerDependencies": {
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.18.8",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "global-tsconfig": "workspace:*",
    "linter-config": "workspace:*",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-icons": "^1.3.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "radix-ui": "latest",
    "react-day-picker": "^9.11.0",
    "react-hook-form": "^7.63.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.11"
  }
}