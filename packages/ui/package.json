{
  "name": "@repo/ui",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components/*": {
      "types": "./src/components/ui/*.tsx",
      "import": "./src/components/ui/*.tsx"
    },
    "./lib/*": {
      "types": "./src/lib/*.ts",
      "import": "./src/lib/*.ts"
    },
    "./styles.css": "./src/styles/globals.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "start-server-and-test storybook http://127.0.0.1:6006 \"pnpm exec test-storybook\""
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-links": "^10.0.8",
    "@storybook/addon-themes": "^10.0.8",
    "@storybook/react": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@storybook/test-runner": "^0.24.1",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.6",
    "@types/react-dom": "19.2.3",
    "global-tsconfig": "workspace:*",
    "linter-config": "workspace:*",
    "storybook": "^10.0.8",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.554.0",
    "radix-ui": "latest",
    "react-day-picker": "^9.11.1",
    "react-hook-form": "^7.66.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0"
  }
}